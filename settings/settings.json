{
    "interface": {
        //
        // グラフの更新頻度（秒：0を指定すると即時更新）
        "update_interval_sec": 0.1,
        //
        // グラフの表示データ件数（件）
        "graph_max_display": 100
    },
    "data_logging": {
        //
        // データログの出力先フォルダー名
        "data_log_dir": "store",
        //
        // データログの出力先ファイル名（日時と拡張子が自動的に設定されます。例：log_20220101_1010.csv）
        "data_log_filename": "log"
    },
    "data_list": {
        // 取り扱うデータのリスト
        /*
            "数値の名前(半角英数字)": {
                "display_name": "インターフェイスに表示される名前(日本語可)",
                "unit": "単位",
                "safe_range_min": [警告を発するしきい値],
                "safe_range_max": [警告を発するしきい値],
            },
        */
        "battery_v": {
            "display_name": "バッテリー電圧",
            "unit": "V",
            "safe_range_min": -10000,
            "safe_range_max": 10000
        },
        "battery_a": {
            "display_name": "バッテリー残量",
            "unit": "A",
            "safe_range_min": 0,
            "safe_range_max": 2
        },
        "battery_temp": {
            "display_name": "バッテリー温度",
            "unit": "℃",
            "safe_range_min": -10000,
            "safe_range_max": 10000
        },
        "body_temp": {
            "display_name": "機体温度",
            "unit": "℃",
            "safe_range_min": -10000,
            "safe_range_max": 10000
        },
        "speed": {
            "display_name": "速度",
            "unit": "km/h",
            "safe_range_min": -10000,
            "safe_range_max": 10000
        },
        "accelerator": {
            "display_name": "アクセル",
            "unit": "%",
            "safe_range_min": -10000,
            "safe_range_max": 10000
        },
        "break": {
            "display_name": "ブレーキ",
            "unit": "%",
            "safe_range_min": -10000,
            "safe_range_max": 10000
        }
    }
}